<%= form_for(@coder) do |f| %>
  <%= f.label :first_name %><br>
  <%= f.text_field :first_name %><br>

  <%= f.label :last_name %><br>
  <%= f.text_field :last_name %><br>

  <%= f.label :about %><br>
  <%= f.text_area :about %><br>

  <%= f.label :looking_for %><br>
  <%= f.text_area :looking_for %><br>

  <%= f.label :image_path %><br>
  <%= f.text_field :img_url %><br>

  <%= f.submit %>
<% end %>
